@{
    var id = ViewBag.Id;
}
<a href="/Student/AddCourse?id=@id">Enroll Course</a>

<h3>DATABASE Current Enrollment</h3>

<div id="divEnrollmentContent">
    <table border="1">
        <thead>
            <tr>
                <th>Course Title</th>
                <th>Instructor</th>
                <th>Year</th>
                <th>Quarter</th>
                <th>Session</th>
                <th>Day</th>
                <th>Time</th>
                <th>Grade</th>
            </tr>
        </thead>
        <tbody data-bind="foreach: viewModel">
            <tr>
                <td data-bind="text: coursetitle"></td>
                <td data-bind="text: instruct"></td>
                <td data-bind="text: year"></td>
                <td data-bind="text: quarter"></td>
                <td data-bind="text: session"></td>
                <td data-bind="text: day"></td>
                <td data-bind="text: time"></td>
                <td data-bind="text: grade"></td>
                <td><div data-bind="template: { name: 'action-template', data: $data}"></div></td>
            </tr>
        </tbody>
    </table>
</div>

<h3>Current Enrollment</h3>
<table border="1">
    <thead>
        <tr>
            <th>Class</th>
            <th>Instructor</th>
            <th>Session</th>
            <th>Schedule</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>CSE 136</td>
            <td>Isaac Chu</td>
            <td>Summer Session 2</td>
            <td>Mon 6-8, Wed 6-8</td>
        </tr>
    </tbody>
</table>

<h3>Courses Available</h3>
<table border="1">
    <thead>
        <tr>
            <th>Class</th>
            <th>Instructor</th>
            <th>Session</th>
            <th>Schedule</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>CSE 134a</td>
            <td>Isaac Chu</td>
            <td>Summer Session 1</td>
            <td>Mon 6-8, Wed 6-8, Thurs 6-8</td>
        </tr>
    </tbody>
</table>

<script type="text/html" id="action-template">
    <button data-bind="DropCourse: $data">Drop</button>
</script>

<script type="text/javascript">
    $(document).ready(function () {
        var studentViewModelObj = new StudentViewModel();
        studentViewModelObj.GetEnrollment("@id");
    });
</script>